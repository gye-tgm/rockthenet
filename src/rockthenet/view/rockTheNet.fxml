<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rockthenet.view.Controller">
    <top>
        <MenuBar BorderPane.alignment="CENTER">
            <menus>
                <Menu mnemonicParsing="false" text="Connect">
                    <items>
                        <MenuItem fx:id="newConnection" mnemonicParsing="false" text="New Connection..." />
                        <Menu mnemonicParsing="false" text="Open Recent">
                            <items>
                                <MenuItem mnemonicParsing="false" text="Juniper NetScreen 5GT" />
                            </items>
                        </Menu>
                    </items>
                </Menu>
                <Menu fx:id="menuBar" mnemonicParsing="false" text="Options">
                    <items>
                        <MenuItem fx:id="settings" mnemonicParsing="false" text="Settings..." />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem fx:id="about" mnemonicParsing="false" text="About" />
                        <MenuItem fx:id="help" mnemonicParsing="false" text="Help" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <bottom>
        <LineChart fx:id="lineChart" prefHeight="88.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <xAxis>
                <!--<CategoryAxis label="time passed" prefHeight="0.0" prefWidth="743.0" side="BOTTOM" tickLength="1.0" /> -->
                <NumberAxis id="xAxis" label="Seconds" side="BOTTOM" />
            </xAxis>
            <yAxis>
                <NumberAxis label="bytes/sec" side="LEFT" />
            </yAxis>
        </LineChart>
    </bottom>
   <center>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <center>
            <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <content>
                      <TableView>
                          <columns>
                              <TableColumn prefWidth="149.0" text="Name" />
                              <TableColumn prefWidth="149.0" text="Source-Zone" />
                              <TableColumn prefWidth="161.0" text="Destination-Zone" />
                              <TableColumn prefWidth="159.0" text="Source-Address" />
                              <TableColumn minWidth="0.0" prefWidth="160.0" text="Destination-Address" />
                              <TableColumn minWidth="0.0" prefWidth="122.0" text="Service" />
                              <TableColumn minWidth="0.0" prefWidth="127.0" text="Action" />
                              <TableColumn minWidth="0.0" prefWidth="127.0" text="Enabled" />
                          </columns>
                      </TableView>
               </content>
            </ScrollPane>
         </center>
         <bottom>
            <BorderPane BorderPane.alignment="CENTER">
               <right>
                  <Button fx:id="refreshButton" alignment="TOP_RIGHT" onAction="#refreshButtonPressed" mnemonicParsing="false" text="Refresh" BorderPane.alignment="CENTER" />
               </right>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </BorderPane>
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
